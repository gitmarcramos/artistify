<form class="form" action="/dashboard/styles/update/{{id}}" method="post">
<label class="label" for="name">Name</label>
<input class="input" type="text" name="name" id="name" value="{{music.name}}">
<label class="label" for="color">Color</label>
<input class="input" type="text" name="color" id="color" value="{{music.color}}">
<label class="label" for="color">Wiki-url</label>
<input class="input" type="text" name="wikiURL" id="wiki-url" value="{{music.wikiURL}}">
